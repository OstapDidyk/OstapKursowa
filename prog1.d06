{*Prog1*}
Program prog1;
Start
Var Int_4 aAAAAAAAA,bBBBBBBBB,xXXXXXXXX,yYYYYYYYY;
Scan aAAAAAAAA;
Scan bBBBBBBBB;
Print aAAAAAAAA ++ bBBBBBBBB;
